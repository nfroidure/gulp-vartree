    "url": "http://www.insertafter.com/blog.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/gulp-vartree.git"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/gulp-vartree/issues"
  },
  "licenses": [
