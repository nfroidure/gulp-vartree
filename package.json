    }
  ],
  "dependencies": {
    "gulp-util": "~3.0.6",
    "readable-stream": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "coveralls": "~2.11.2",
    "gulp": "~3.9.0",
    "gulp-mdvars": "2.0.0",
    "istanbul": "~0.3.17",
    "mocha": "~2.2.5",
    "mocha-lcov-reporter": "0.0.2",
