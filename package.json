{
  "name": "gulp-vartree",
  "version": "1.0.0",
  "description": "Create a variable tree from the property values of given files.",
  "main": "src/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*.mocha.js",
